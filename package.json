{
  "name": "blow-dry",
  "version": "0.0.5",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements"
  ],
  "description": "Turn some (server) expanded HTML into the smallest, optimized HTML representation so it can be rapidly cloned (via web components, for example) on the client",
  "main": "blow-dry.js",
  "module": "blow-dry.js",
  "exports": {
    ".": "./blow-dry.js",
    "./blow-dry.js": "./blow-dry.js"
  },
  "scripts": {
    "serve": "node node_modules/may-it-serve/serve.js",
    "update": "ncu -u && npm install"
  },
  "devDependencies": {
    "may-it-serve": "0.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/blow-dry.git"
